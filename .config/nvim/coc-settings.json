{
  "eslint.format.enable": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "eslint.workingDirectories": [{"mode": "auto"}],
